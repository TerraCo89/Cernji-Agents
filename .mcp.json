{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "sqlite-resume": {
      "type": "stdio",
      "command": "cmd",
      "args": [
          "/c",
          "npx",
          "-y",
          "mcp-sqlite",
          "data\\resume_agent.db"
      ]
    },
    "sqlite-japanese": {
      "type": "stdio",
      "command": "cmd",
      "args": [
          "/c",
          "npx",
          "-y",
          "mcp-sqlite",
          "data\\japanese_agent.db"
      ]
    },
    "qdrant-vectors": {
      "type": "stdio",
      "command": "cmd",
      "args": [
          "/c",
          "uvx",
          "mcp-server-qdrant"
      ],
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "COLLECTION_NAME": "resume-agent-chunks",
        "EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2"
      }
    },
    "resume-agent-langgraph": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "cd /d D:\\source\\Cernji-Agents\\apps\\resume-agent-langgraph && uv run resume_agent_langgraph.py"
      ],
      "env": {
        "ANTHROPIC_API_KEY": "YOUR_API_KEY_HERE",
        "STORAGE_BACKEND": "sqlite",
        "OBSERVABILITY_SERVER_URL": "http://localhost:3001"
      }
    }
  }
}
